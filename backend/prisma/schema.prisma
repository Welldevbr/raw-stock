generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Product {
  id    Int     @id @default(autoincrement())
  code  String  @unique
  name  String
  price Float
  createdAt     DateTime @default(now())
}

model RawMaterial {
  id               Int                     @id @default(autoincrement())
  code             String                  @unique
  name             String
  stockQuantity    Int
}